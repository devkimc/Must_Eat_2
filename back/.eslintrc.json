{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "rules": {
        "import/no-unresolved": "off",
        "react/prop-types": 0,
        "import/no-cycle": "off",
        "consistent-return": "off"
    }
}
